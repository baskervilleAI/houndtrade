{
  "name": "houndtrade",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:lambdas": "cd backend && npm run build",
    "deploy:dev": "cd backend && ./scripts/deploy.sh dev",
    "deploy:prod": "cd backend && ./scripts/deploy.sh prod",
    "build:web": "expo export --platform web --output-dir dist",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:all": "npm run build:web && npm run build:android && npm run build:ios",
    "predeploy": "npm run build:web",
    "deploy:github": "gh-pages -d dist"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~54.0.9",
    "expo-constants": "~18.0.9",
    "expo-status-bar": "~3.0.8",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "^13.15.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^4.4.7"
  },
  "homepage": "https://baskervilleAI.github.io/houndtrade",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@expo/cli": "^54.0.7",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~54.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2",
    "ws": "^8.18.3"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
