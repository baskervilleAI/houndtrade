{
  "name": "houndtrade",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:lambdas": "cd backend && npm run build",
    "deploy:dev": "cd backend && ./scripts/deploy.sh dev",
    "deploy:prod": "cd backend && ./scripts/deploy.sh prod"
  },
  "dependencies": {
    "expo": "~54.0.9",
    "expo-status-bar": "~3.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.81.4",
    "react-native-web": "~0.19.0",
    "expo-constants": "~17.0.3"
  },
  "devDependencies": {
    "@types/react": "~18.2.0",
    "typescript": "~5.9.2",
    "@babel/core": "^7.23.7",
    "babel-preset-expo": "~11.0.0"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
